{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
   "functions": {
    "wsgi.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "/var/task/app"
  },
  "installCommand": "pip install -r requirements.txt"
}
